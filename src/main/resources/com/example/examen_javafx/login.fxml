         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="champLogin" layoutX="272.0" layoutY="156.0" prefHeight="25.0" prefWidth="281.0" promptText="nom utilisateur" />
      <PasswordField fx:id="champPassword" layoutX="272.0" layoutY="229.0" prefHeight="25.0" prefWidth="281.0" promptText="mot de passe" />
      <Label layoutX="272.0" layoutY="202.0" text="Mot de Passe">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Label>
      <Button layoutX="310.0" layoutY="272.0" mnemonicParsing="false" onAction="#btnConnect" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: #E96151;" text="Se Connecter" textFill="#fbf0f0" />
      <Button layoutX="425.0" layoutY="272.0" mnemonicParsing="false" onAction="#btnRegister" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: #E96151;" text="S'inscrire" textFill="#f8f0f0" />
